# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Google Gemini API Keys
# Get your API keys from https://aistudio.google.com/
# Note: API keys should start with 'AIza' for validation
GEMINI_API_KEY=your_gemini_api_key_here              # Main API key for LexiBot
GEMINI_API_KEY_1=your_gemini_api_key_1_here          # API key for document processing

# Server Configuration (Optional - defaults are set if not specified)
NODE_ENV=development                                  # Options: development, production
PORT=3000                                             # Main server port (default: 3000)
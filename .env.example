# Jura Application Environment Variables
# Copy this file to .env and fill in your actual values
# This file is used by docker-compose.yml

# Google Gemini API Keys
# Get your API keys from https://aistudio.google.com/
# Note: API keys should start with 'AIza' for validation
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_API_KEY_1=your_gemini_api_key_1_here

# Backend Configuration
NODE_ENV=development

# Frontend Configuration
# These are used by the React app to connect to the backend
# For Docker: use service names (backend) instead of localhost
# For local development: use localhost
VITE_API_URL=http://localhost:3000
VITE_LEXI_API_URL=http://localhost:5000
